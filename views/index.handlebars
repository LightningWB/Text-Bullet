    <script>
        (function () {
            //document.getElementById("adblock-msg").style.display = window.adsEnabled !== undefined ? "none" : "";

            if (window.innerWidth <= 1200) {
                MOBILE.is = true;
                MOBILE.which("you");
            }

            document.getElementById("init-cover").style.display = "none";

            ENGINE.playersOnline = parseInt(document.getElementById("autocount").value);
            ENGINE.version = {{{version}}};

            document.getElementById("game-info").innerHTML = ENGINE.setOnline() + "<div id='game-peaks' style='background-color: rgba(0,0,0,0);font-size: 12px;margin-top:10px'>daily peak <b>" + document.getElementById("auto_dailyPeak").value + "</b> \u2022 alltime peak <b>" + document.getElementById("auto_alltimePeak").value + "</b></div>";
            
            if (document.getElementById("autologin").value !== "0") {
                document.getElementById("game-info").setAttribute("style", "color:white !important");
                document.getElementById("game-peaks").style.color = "white";
                SOCKET.autolog();
            }

            //load latest changelog
            ENGINE.ajaxCall(
                "/changelog.aspx/GetSpecificChangelog",
                { "ver": -1 },
                function (r) {
                    let job = JSON.parse(r),
                        body = OTHER.markdown(job.body),
                        leaders = job.leaders,
                        ltext = "";

                    if (body.length > 400) {
                        body = body.substr(0, 420);
                        let space = body.lastIndexOf(" "),
                            comma = body.lastIndexOf(","),
                            period = body.lastIndexOf(".");

                        if (space !== -1 && space > 400) body = body.substr(0, space);
                        else if (comma !== -1 && comma > 400) body = body.substr(0, comma);
                        else if (period !== -1 && space > 400) body = body.substr(0, period);

                        body += "...";
                    }
                    body += "<br /><br /><a target='_blank' class='floating-box-link' href='./changelog'>[view changelog]</a>";
                    
                    document.getElementById("changelog-date").innerHTML = job.title + " \u2022 " + job.date;
                    document.getElementById("changelog-desc").innerHTML = body;
                    
                    leaders.sort(function (a, b) {
                        return b.xp - a.xp;
                    });
                    for (let i = 0; i < 5; i++) {
                        if (leaders[i] === undefined) break;
                        ltext += "<p class='lboard-wide clearback'>" + (i + 1) + ")</p><span class='lboard-main'><b>" + leaders[i].username + "</b>: level " + (leaders[i].level + 1) + "</span><br /><span class='lboard-sub'>" + leaders[i].xp + " total xp</span><div class='clearback' style='height:5px'></div>";
                    }

                    document.getElementById("leaderboard-list").innerHTML = ltext + "<br /><a target='_blank' class='floating-box-link' href='./leaderboard'>[view leaderboard]</a>";
                }
            );

            if (ENGINE.isProd()) {
                console.log("\n\n\nWARNING! The Travelers has the developer's blessing to be used for mods, but if someone has linked you to install one, PLEASE make absolutely certain the source is trustworthy, or read through the JS yourself. " +
                    "XSS (cross-site scripting) is real and incredibly dangerous, and injections can even occur in seemingly harmless places, like the 'src' attribute in the 'img' tag if you want a fancy image for a texturepack. When in doubt, simply do not install the mod at all.\n\n\n");
            }

			
			document.onreadystatechange = function(e)
			{
				if(e.eventPhase !== 2)return;
				{{{worldGen}}}
			}
        })();
		
		function getCaptcha(r) {
			SOCKET.captcha = r;
		}
    </script>
	<script>
		{{{getPatchScript}}}
	</script>
</body>
</html>
