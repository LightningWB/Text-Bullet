            <!-- darkmode -->
            <div class="settings-sect" onclick="SETTINGS.toggleDarkMode();" style="cursor:pointer;">
                <div class="settings-check" id="set-darkmode"></div> toggle darkmode
            </div>
            <br />

            <!-- disable hover text-->
            <div class="settings-sect" onclick="SETTINGS.toggleHover();" style="cursor:pointer;">
                <div class="settings-check settings-checked" id="set-hover"></div> toggle hover-text
            </div>
            <br />

            <!-- autowalk past-->
            <div class="settings-sect">
                <div class="settings-check settings-checked" id="set-autowalkpast" onclick="SETTINGS.toggleAutoPast();"></div><span onclick="SETTINGS.toggleAutoPast();" style="cursor:pointer;"> auto-walk past events after </span><input id="set-autowalkpast-time" type="number" min="1" max="100" value="30" onchange="SETTINGS.setAutopastTime(this.value);" /><span onclick="SETTINGS.toggleAutoPast();" style="cursor:pointer;"> cycles</span>
            </div>
            <br />

            <!-- notifications-->
            <div class="settings-sect" onclick="SETTINGS.toggleNotifAny();" style="cursor:pointer;">
                <div class="settings-check settings-checked" id="set-notifAny"></div> enable notifications
            </div>
            <br />
            <div id="notif-sub0000" class="settings-sect" style="cursor:pointer;margin-left: 40px;">
                notifications can activate after <input id="set-notification-timeout" type="number" min="1" value="10" onchange="SETTINGS.setNotifTimeout(this.value);" /> cycles without inputs
            </div>
            <br />
            <div id="notif-sub0" class="settings-sect" onclick="SETTINGS.toggleNotifSound();" style="cursor:pointer;margin-left: 40px;">
                <div class="settings-check settings-checked" id="set-notifSound"></div> enable notification sound effect
            </div>
            <br />
            <div id="notif-sub00" class="settings-sect" onclick="SETTINGS.toggleNotifDesktop();" style="cursor:pointer;margin-left: 40px;">
                <div class="settings-check settings-checked" id="set-notifDesktop"></div> enable desktop notifications
            </div>
            <br />
            <div id="notif-sub1" class="settings-sect" onclick="SETTINGS.toggleNotifTraveler();" style="cursor:pointer;margin-left: 40px;">
